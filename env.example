# Database Configuration
# SQLite is used by default - no configuration needed
# Database file: server/storage/midot_traffic.db

# Server Configuration
PORT=3001
NODE_ENV=development

# MIDOT API Configuration (if needed in future)
# MIDOT_API_KEY=your_api_key_here
# MIDOT_API_BASE_URL=https://mdotjboss.state.mi.us/MiDrive/camera

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash

# Batch Processing Configuration
GEMINI_USE_BATCH_MODE=true
GEMINI_BATCH_SIZE=10
GEMINI_BATCH_POLLING_INTERVAL=30000

# Processing Configuration
GEMINI_CONFIDENCE_THRESHOLD=0.7
GEMINI_MAX_IMAGES_PER_BATCH=20
GEMINI_MAX_RETRIES=3

# Rate Limiting Configuration
GEMINI_RATE_LIMIT_DELAY=2000
GEMINI_MAX_CONCURRENT_REQUESTS=5

# Safety Settings (BLOCK_NONE, BLOCK_LOW_AND_ABOVE, BLOCK_MEDIUM_AND_ABOVE, BLOCK_HIGH_AND_ABOVE)
GEMINI_SAFETY_HARASSMENT=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_HATE_SPEECH=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_SEXUALLY_EXPLICIT=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_DANGEROUS_CONTENT=BLOCK_MEDIUM_AND_ABOVE

# Content Filtering
GEMINI_ENABLE_CONTENT_FILTERING=true
GEMINI_MAX_RESPONSE_LENGTH=10000

# Error Handling
GEMINI_ENABLE_RETRY_ON_FAILURE=true
GEMINI_RETRY_BACKOFF_MULTIPLIER=2.0
GEMINI_MAX_RETRY_DELAY=30000

# Monitoring and Logging
GEMINI_ENABLE_DETAILED_LOGGING=false
GEMINI_LOG_PROCESSING_METRICS=true
GEMINI_METRICS_RETENTION_HOURS=24

# Image Storage Configuration
# IMAGE_STORAGE_PATH=./server/storage/images
# MAX_IMAGE_SIZE=10485760
# IMAGE_RETENTION_DAYS=30

# Analytics Configuration
# VEHICLE_COUNT_AGGREGATION_INTERVAL=600000
# Time interval in milliseconds for grouping vehicle count data in charts
# Default: 600000 (10 minutes)
# Range: 60000 (1 minute) to 3600000 (60 minutes)
